{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "signalRService": {}
  },
  "http": {
    "cors": {
      "allowedOrigins": [
        "https://portal.azure.com",
        "http://localhost:8080",
        "http://127.0.0.1:5500",
        "http://localhost:5500",
        "http://localhost:5500/content/",
        "https://localhost:5500/content/"
      ],
      "supportedMethods": [ "GET", "POST", "OPTIONS" ],
      "supportedHeaders": [ "x-ms-signalr-user", "x-ms-signalr-access-token" ]
    }
  }
}